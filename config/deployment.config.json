{
  "server": {
    "host": "hostname",
    "isSecure": false,
    "prefix": "hdr",
    "headers": {
      "hdr-usr": "domain\\username",
      "cache-control": "no-cache"
    },
    "addParams": {
      "withCredentials": false
    }
  },
  "desktop": {
    "destination": "C:\\Users\\%username%\\Documents\\Qlik\\Sense\\Extensions"
  }
}